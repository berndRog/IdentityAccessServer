{
  "AuthServer": {
    "IssuerUri": "https://localhost:7001/",
    "ScopeApi": "api",
    "Tokens": {
      "EncryptAccessTokens": false
    },

    "BlazorWasm": {
      "ClientId": "blazor-wasm",
      "BaseUrl": "https://localhost:6010",
      "RedirectPath": "/authentication/login-callback",
      "PostLogoutRedirectPath": "/authentication/logout-callback",
      "Type": "Public"
    },

    "WebMvc": {
      "ClientId": "webclient-mvc",
      "BaseUrl": "https://localhost:6020",
      "RedirectPath": "/signin-oidc",
      "PostLogoutRedirectPath": "/signout-callback-oidc",
      "Type": "Confidential"
    },

    "Android": {
      "ClientId": "android-client",
      "CustomSchemeRedirectUriString": "com.rogallab.oidc:/callback",
      "LoopbackRedirectUriString": "http://127.0.0.1:8765/callback",
      "PostLogoutRedirectUriString": "com.rogallab.oidc:/logout-callback",
      "Type": "Public"
    },

    "ServiceClient": {
      "ClientId": "service-client",
      "Type": "Confidential"
    }
  }
}